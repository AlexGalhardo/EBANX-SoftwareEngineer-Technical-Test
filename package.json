{
  "name": "ebanx-softwareenginer-technical-test",
  "version": "1.0.0",
  "description": "technical test for the ebanx software engineer selection process",
  "main": "src/server.ts",
  "engines": {
    "node": "v16.14.2",
    "npm": "8.5.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --exit-child --transpile-only --ignore-watch node_modules src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --passWithNoTests --runInBand --coverage",
    "build": "tsc --project ./",
    "fix:prettier": "npx prettier --write src/*.ts src/**/*.ts src/**/**/*.ts src/**/**/**/*.ts tests/**/*.ts",
    "fix:lint": "npx eslint . --fix",
    "precommit": "npm run fix:prettier && npm run fix:lint",
    "prepare": "husky install"
  },
  "keywords": [
    "typescript",
    "api"
  ],
  "author": "Alex Galhardo <aleexgvieira@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-jest": "^28.1.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
